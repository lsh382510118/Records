{
  "name": "MoonYa-records",
  "version": "1.0.0",
  "description": "a project of react server render",
  "main": "App.jsx",
  "scripts": {
    "build:client": "cross-env NODE_ENV=production webpack --config ./build/webpack.prod.conf.js",
    "build:server": "cross-env NODE_ENV=production webpack --config ./build/webpack.server.conf.js",
    "build": "npm run clear && npm run build:client && npm run build:server",
    "clear": "rimraf dist",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --open --config ./build/webpack.client.conf.js",
    "dev:mock": "cross-env NODE_ENV=mock webpack-dev-server --open --config ./build/webpack.client.conf.js",
    "dev:server": "cross-env NODE_ENV=development nodemon server/server.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "start": "cross-env NODE_ENV=production nodemon server/server.js",
    "lint": "npx eslint src client/pages"
  },
  "author": "Moonya",
  "license": "ISC",
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
